{
  // Default platform definitions for Augent
  // This file contains all 17 supported AI coding platforms
  // Each platform has detection patterns and transformation rules
  "platforms": [
    // Antigravity
    {
      "id": "antigravity",
      "name": "Google Antigravity",
      "directory": ".agent",
      "detection": [".agent"],
      "transforms": [
        {
          "from": "rules/**/*.md",
          "to": ".agent/rules/**/*.md"
        },
        {
          "from": "commands/**/*.md",
          "to": ".agent/workflows/**/*.md"
        },
        {
          "from": "skills/**/SKILL.md",
          "to": ".agent/skills/{name}/SKILL.md"
        },
        {
          "from": "skills/**/*",
          "to": ".agent/skills/{name}/**/*"
        }
      ]
    },
    // Augment Code
    {
      "id": "augment",
      "name": "Augment Code",
      "directory": ".augment",
      "detection": [".augment"],
      "transforms": [
        {
          "from": "rules/**/*.md",
          "to": ".augment/rules/**/*.md"
        },
        {
          "from": "commands/**/*.md",
          "to": ".augment/commands/**/*.md"
        }
      ]
    },
    // Claude Code
    {
      "id": "claude",
      "name": "Claude Code",
      "directory": ".claude",
      "detection": [".claude", "CLAUDE.md"],
      "transforms": [
        {
          "from": "commands/**/*.md",
          "to": ".claude/commands/**/*.md"
        },
        {
          "from": "rules/**/*.md",
          "to": ".claude/rules/**/*.md"
        },
        {
          "from": "agents/**/*.md",
          "to": ".claude/agents/**/*.md"
        },
        {
          "from": "skills/**/SKILL.md",
          "to": ".claude/skills/{name}/SKILL.md"
        },
        {
          "from": "skills/**/*",
          "to": ".claude/skills/{name}/**/*"
        },
        {
          "from": "mcp.jsonc",
          "to": ".mcp.json",
          "merge": "deep"
        },
        {
          "from": "AGENTS.md",
          "to": "CLAUDE.md",
          "merge": "composite"
        }
      ]
    },
    // Claude Plugin
    {
      "id": "claude-plugin",
